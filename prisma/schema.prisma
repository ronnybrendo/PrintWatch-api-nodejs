generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model central {
  id           Int      @id @default(autoincrement())
  data         DateTime
  hora         String
  empresa      Int
  setor        String
  usuario      String
  paginas      Int
  copias       Int
  impressora   String
  nomearquivo  String
  tipo         String
  tipopage     String
  cor          String
  tamanho      String
  nomepc       String
  ip           String
  mac          String
}